programa
{

inteiro n1
inteiro n2
	
	funcao inicio()
	{
		escreva("Me de um valor \n" )
		leia(n1)
		escreva("Me de outro valor \n")
		leia(n2)

		se(n1 > n2){
			escreva(n1)
		enquanto(n1 > n2)
         escreva( n1 = n1 - 1)
	 
		}
		se(n1 < n2){
			escreva( n1)
		enquanto(n1 < n2)
          escreva( n1 = n1 + 1)
	 
		}
		
	}
}
